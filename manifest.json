{
  "manifest_version": 3,
  "name": "Forkie",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["http://open.spotify.com/*", "https://open.spotify.com/*"],
      "js": ["content.js"]
    }
  ]
}
